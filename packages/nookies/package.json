{
  "name": "nookies",
  "description": "A set of cookie helpers for Next.js",
  "version": "0.0.0-semantic-release",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "maticzav/nookies",
  "author": {
    "name": "Matic Zavadlal",
    "email": "matic.zavadlal@gmail.com"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn g:tsc -d && yarn run minify",
    "minify": "terser -c -m --source-map \"content='./dist/index.js.map',url='index.min.js.map'\" -o dist/index.min.js -- dist/index.js"
  },
  "dependencies": {
    "cookie": "^0.7.0",
    "set-cookie-parser": "^2.4.6"
  },
  "devDependencies": {
    "@types/cookie": "0.5.1",
    "@types/express": "4.17.25",
    "@types/next": "9.0.0",
    "@types/node": "14.18.63",
    "@types/set-cookie-parser": "2.4.10",
    "terser": "5.14.2"
  },
  "keywords": [
    "cookie",
    "cookies",
    "next",
    "nextjs",
    "js",
    "auth",
    "destory",
    "parse",
    "create",
    "nookies"
  ],
  "release": {
    "branch": "master"
  },
  "license": "MIT"
}
